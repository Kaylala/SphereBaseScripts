[ITEMDEF iChestTreasure]
ID=iChestMetal

ON=@CREATE
ATTR=attr_static|attr_move_never
TYPE=T_CONTAINER_LOCKED
COLOR=045e
MORE1=01
MORE2=01 // the level of the chest.
TIMER=900

ON=@DCLICK
if (<MORE1>==02)
if (<TIMER>>700)
SRC.MESSAGE <def.msg_cantOpenYet>
RETURN 1
else
TYPE=T_CONTAINER
ATTR=attr_move_never|attr_decay //activate decay after opening
TIMER=650
RETURN 0
endif
//***Creating Monster on Opening of Treasure Chest depending on Map Difficulty
elif (<MORE1>==01)
MORE1=02
TIMER=900
SOUND=01FE
IF <MORE2>==01
NEWITEM=iTreasSpawnVeasy //Treasure map random very easy monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnVeasy //Treasure map random very easy monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnVeasy //Treasure map random very easy monster spawn
NEW.P=<P>
RETURN 1
elif <MORE2>==02
NEWITEM=iTreasSpawnEasy //Treasure map random easy monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnEasy //Treasure map random easy monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnEasy //Treasure map random easy monster spawn
NEW.P=<P>
RETURN 1
elif <MORE2>==03
NEWITEM=iTreasSpawnAvg //Treasure map random average monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnAvg //Treasure map random average monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnAvg //Treasure map random average monster spawn
NEW.P=<P>
RETURN 1
elif <MORE2>==04
NEWITEM=iTreasSpawnHard //Treasure map random hard monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnHard //Treasure map random hard monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnHard //Treasure map random hard monster spawn
NEW.P=<P>
RETURN 1
elif <MORE2>==05
NEWITEM=iTreasSpawnVhard //Treasure map random very hard monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnVhard //Treasure map random very hard monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnVhard //Treasure map random very hard monster spawn
NEW.P=<P>
RETURN 1
endif
RETURN 0
endif
