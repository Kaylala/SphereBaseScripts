[TYPEDEF T_L1MAP_CHEST]
//LEVEL1MAP Treasure map Chest Spawn
ON=@DCLICK
IF (<DISTANCE>>3)
RETURN 1
endif
IF (<LINK.TOPOBJ.UID> != <SRC.UID>)
RETURN 1
endif
IF ((<SRC.FINDLAYER(layer_hand1).BASEID>==iPickaxe) && (<SRC.MINING>>50.0))
SRC.ANIM 0B
SOUND 0126
NEWITEM TM_LOOT_1 //Treasure map random poor loot
NEW.P=<P>
SRC.MESSAGE=<def.msg_dig_treasure>
LINK.REMOVE //remove linked deciphered map
NEWITEM=iTreasSpawnVeasy //Treasure map very easy monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnVeasy //Treasure map very easy monster spawn
NEW.P=<P>
NEWITEM=iTreasSpawnVeasy //Treasure map very easy monster spawn
NEW.P=<P>
REMOVE //remove trigger plant
RETURN 1
endif
RETURN 1

